year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{rmaxent}: Tools for working with Maxent in R",
         author = c(person("John", "Baumgartner"),
                    person("Peter", "Wilson")),
         year = year,
         note = note,
         url = "https://github.com/johnbaums/rmaxent")
